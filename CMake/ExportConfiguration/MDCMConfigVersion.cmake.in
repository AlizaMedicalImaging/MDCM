set(MDCM_MAJOR_VERSION "@MDCM_MAJOR_VERSION@")
set(MDCM_MINOR_VERSION "@MDCM_MINOR_VERSION@")
set(MDCM_BUILD_VERSION "@MDCM_BUILD_VERSION@")
set(MDCM_VERSION       "@MDCM_VERSION@")

set(PACKAGE_VERSION "${MDCM_VERSION}")
if("${PACKAGE_FIND_VERSION}" STREQUAL "")
  set(PACKAGE_VERSION_COMPATIBLE 1)
elseif("${PACKAGE_FIND_VERSION}" VERSION_EQUAL "${PACKAGE_VERSION}")
  set(PACKAGE_VERSION_COMPATIBLE 1)
  set(PACKAGE_VERSION_EXACT 1)
elseif("${PACKAGE_FIND_VERSION}" VERSION_LESS "${PACKAGE_VERSION}")
  set(PACKAGE_VERSION_COMPATIBLE 1)
else()
  message("REQUESTING: [${PACKAGE_FIND_VERSION}] but found: ${PACKAGE_VERSION}")
endif()
